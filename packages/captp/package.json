{
  "name": "@endo/captp",
  "version": "2.0.7",
  "description": "Capability Transfer Protocol for distributed objects",
  "type": "module",
  "keywords": [
    "agoric",
    "captp",
    "eventual-send"
  ],
  "author": "Endo contributors",
  "homepage": "https://github.com/endojs/endo#readme",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "module": "src/index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git"
  },
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-check": "yarn lint",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint": "yarn lint:eslint && yarn lint:types",
    "lint:eslint": "eslint '**/*.js'",
    "lint:types": "tsc -p jsconfig.json"
  },
  "devDependencies": {
    "@endo/init": "^0.5.41",
    "@endo/ses-ava": "^0.2.25",
    "ava": "^3.12.1",
    "c8": "^7.7.3"
  },
  "dependencies": {
    "@endo/eventual-send": "^0.15.3",
    "@endo/marshal": "^0.6.7",
    "@endo/nat": "^4.1.12",
    "@endo/promise-kit": "^0.2.41"
  },
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
